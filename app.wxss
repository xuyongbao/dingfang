/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view {
    font-family: "PingFangSC-Medium"
}
image{
  height:100%;
  width:100%;
}
.hidden{
  display: none!important;
}

.clear{clear:both;height:0;overflow:hidden;}
.ellipse1{display: -webkit-box;overflow: hidden; white-space: normal;text-overflow: ellipsis; word-wrap: break-word;-webkit-line-clamp: 1;-webkit-box-orient: vertical;}
.ellipse2{display: -webkit-box; overflow: hidden;white-space: normal; text-overflow: ellipsis;word-wrap: break-word;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}