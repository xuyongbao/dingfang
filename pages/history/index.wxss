page{
  padding-bottom: 120rpx;
}
.tips{
  padding-top:30rpx;
  font-size:38rpx;
  padding-left:40rpx;
  background:#eee;
  padding-bottom:30rpx;
  padding-right:30rpx;
}
.tips .state_h{
  height:auto;
  line-height: 60rpx;
}
.tips .state_h .state_item{
  width:33%;
}
.tips .item_block{
  width:40rpx;
  height:40rpx;
}
.search{
  background: #F4F4F4;
  height:72rpx;
  width:750rpx;
  padding-top:16rpx;  
}
.search view{
  background: #FFFFFF;
  border-radius: 10rpx;
  width:718rpx;
  height:56rpx;
  margin-left:16rpx;
}
.search icon{
  margin-left:33rpx;
  float: left;
  margin-top:15rpx;
  margin-right:33rpx;
}
.search input{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
}

.calendar{
  display: flex;
  height:120rpx;
  line-height: 120rpx;
  margin-top:40rpx;
}
.item_c{
  flex: 1;
}
.c_left{
  text-align: right;
}
.c_left text , .c_right text{
  display: inline-block;
  height:100%;
  font-size:60rpx;
  padding:0 30rpx;
}
.c_middle{
  flex: 1.4;
  text-align: center;
  line-height: 60rpx;
}
.c_middle text{
  display: block;
}

.nav{
  display: flex;
  text-align: center;
  border-bottom:1px solid #eee;
  border-top:1px solid #eee;
  padding:20rpx 0;
  margin-top:20rpx;
}
.nav_item{
  flex: 1;
  font-size:32rpx;
  display:flex;/*Flex布局*/
  display: -webkit-flex; /* Safari */
  align-items:center;/*指定垂直居中*/
}
.nav_item text{
  width:100%;
  display: block;
}
.nav_item picker{
  width:100%;
  display: block;
}
.l_item{
  display: flex;
  height:120rpx;
  border-bottom:2px solid #eee;
  line-height: 120rpx;
  text-align: center;
  font-size:32rpx;
}
.item_list{
  flex: 1;
}
.lineH{
  padding:20rpx;
}
.lineH text{
  display: block;
  line-height: 40rpx;
}

.no_list{
  margin-top:40rpx;
  text-align: center;
}
.no_list text{
  display: block;
  margin-top:30rpx;
}

.downLoad{
  position: fixed;
  bottom:0;
  left: 0;
  width:100%;
}
.state_h{
  height:80rpx;
  line-height: 80rpx;
}
.state_item{
  float:left;
}
.item_block{
  height:60rpx;
  width:60rpx;
  margin:10rpx;
  float:left;
}
.state_item text{
  float: left;
  font-size:32rpx;
}
.fast{
  line-height: 80rpx;
  font-size:28rpx;
  padding-left:30rpx;
}
.fast text{
  float:left;
}
.fast_click{
  color:blueviolet;
  float:left;
  margin-left:10rpx;
  margin-top:10rpx;
}

.nav{
  display: flex;
  position:fixed;
  bottom:0;
  left:0;
  width:100%;
  background:#eee;
  font-size:28rpx;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
}
.nav_item{
  flex:1;
}























.color1{
  background:greenyellow;
}
.color2{
  background: red;
}
.color3{
  background: orange;
}
.color4{
  background:#c496ef;
}
.see_queue{
  margin:10rpx 0 0 10rpx;
}
