page{
  padding-bottom:100rpx;
}
.tips{
  padding-top:30rpx;
  font-size:30rpx;
  padding-left:40rpx;
  background:#eee;
  padding-bottom:30rpx;
}
.tips text{
  display: block;
}
.house{
  margin-top:30rpx;
}
.house_item{
  width:25%;
  box-sizing: border-box;
  border:1px solid #ccc;
  height:187.5rpx;
  float: left;
  text-align: center;
  font-size:30rpx;
  display:flex;/*Flex布局*/
  display: -webkit-flex; /* Safari */
  align-items:center;/*指定垂直居中*/
}
.content_h{
  width:100%;
}
.content_h view{
  width:100%;
}
.state_h{
  height:80rpx;
  line-height: 80rpx;
}
.state_item{
  float:left;
}
.item_block{
  height:60rpx;
  width:60rpx;
  margin:10rpx;
  float:left;
}
.state_item text{
  float: left;
  font-size:32rpx;
}
.fast{
  line-height: 80rpx;
  font-size:28rpx;
}
.fast_click{
  color:blueviolet;
}

.nav{
  display: flex;
  position:fixed;
  bottom:0;
  left:0;
  width:100%;
  background:#eee;
  font-size:28rpx;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
}
.nav_item{
  flex:1;
}























